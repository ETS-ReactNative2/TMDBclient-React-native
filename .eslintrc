{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "globals": {
      "__DEV__": true,
      "__dirname": false,
      "__fbBatchedBridgeConfig": false,
      "alert": false,
      "cancelAnimationFrame": false,
      "cancelIdleCallback": false,
      "clearImmediate": true,
      "clearInterval": false,
      "clearTimeout": false,
      "console": false,
      "document": false,
      "escape": false,
      "Event": false,
      "EventTarget": false,
      "exports": false,
      "fetch": false,
      "FormData": false,
      "global": false,
      "jest": false,
      "Map": true,
      "module": false,
      "navigator": false,
      "process": false,
      "Promise": true,
      "requestAnimationFrame": true,
      "requestIdleCallback": true,
      "require": false,
      "Set": true,
      "setImmediate": true,
      "setInterval": false,
      "setTimeout": false,
      "window": false,
      "XMLHttpRequest": false,
      "pit": false
    },
    "rules": {
      "comma-dangle": 0,
      "react/jsx-uses-vars": 1,
      "react/display-name": 1,
      "no-unused-vars": "warn",
      "no-console": 1,
      "no-unexpected-multiline": "warn",
      "no-cond-assign": 1,
      "no-underscore-dangle": 0,
      "arrow-body-style": [
        "error",
        "as-needed",
        {
          "requireReturnForObjectLiteral": true
        }
      ],
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "function-paren-newline": 0,
      "global-require": 0,
      "no-bitwise": 0,
      "no-return-assign": 0,
      "func-names": 0,
      "no-else-return": 0,
      "consistent-return": 0,
      "no-mixed-operators": 0,
      "object-curly-newline": [
        "error",
        {
          "multiline": true
        }
      ],
      "class-methods-use-this": 0,
      "max-len": [
        "error",
        {
          "ignoreTemplateLiterals": true,
          "ignoreStrings": true,
          "ignoreComments": true,
          "ignoreRegExpLiterals": true,
          "code": 160
        }
      ],
      "linebreak-style": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/first": 0,
      "import/newline-after-import": 0,
      "import/prefer-default-export": 0,
      "import/order": 0,
      "import/no-extraneous-dependencies": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/sort-comp": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-one-expression-per-line": 0,
      "no-use-before-define": 0,
      "indent": [
        "error",
        4
      ],
      "arrow-parens": 0,
      "react/jsx-indent": [
        2,
        4
      ],
      "react/jsx-indent-props": [
        2,
        4
      ],
      "react/destructuring-assignment": 0,
      "no-restricted-globals": 0,
      "no-prototype-builtins": 0,
      "no-unused-expressions": "off",
      "operator-linebreak": [
        "error",
        "before",
        {
          "overrides": {
            "?": "after",
            ":": "after"
          }
        }
      ]
    }
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "15.6.1"
    }
  }
}